FoamFile
{
version 2.0;
format ascii;
class dictionary;
location "system";
object sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
interpolationScheme cellPoint;
setFormat raw;
sets
(
horizontalLine
{
type uniform;
axis distance;
start ( 0.1 0.0 0.005 );
end ( 0.1 0.0 1.995 );
nPoints 100;
}
);
fields ( p U T);
