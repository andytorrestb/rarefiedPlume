FoamFile
{
version 2.0;
format ascii;
class dictionary;
location "system";
object sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
type sets;

interpolationScheme cellPoint;

setFormat csv;

sets
(
horizontalLine
{
type uniform;
axis distance;
start ( 1.0 1.0 0.0 );
end ( 14.14 14.14 0.0 );
nPoints 100;
}
);
fields (U_Ar Ttra_Ar rhoN_Ar);
